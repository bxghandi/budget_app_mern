{
  "name": "mern_dashboard",
  "version": "1.0.0",
  "description": "Financial dashboard built with MERN stack",
  "main": "server.ts",
  "scripts": {
    "client-install": "npm install --prefix ../client",
    "start": "ts-node-dev server.ts",
    "server": "nodemon --exec ts-node server.ts",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Josh Martinez",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^11.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.3.0",
    "mongoose": "^5.13.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
